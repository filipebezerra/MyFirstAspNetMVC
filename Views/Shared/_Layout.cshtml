<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="siteImage">
        <img src="~/Content/themes/base/images/ASP.NET-MVC-4.jpg" />
    </div>
    
    <div id="siteMenu">
        @Html.ActionLink("Home", "Index", "Home")
        @Html.ActionLink("Galeria", "GalleryView", "Gallery")
        @Html.ActionLink("Contato", "ContactView", "Contact")
        @Html.ActionLink("Meus Contatos", "MyContacts", "Contact")
        @Html.ActionLink("Meu Contatos Tipados", "MyContactsTyped", "Contact")
    </div>

    @RenderBody()    

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
